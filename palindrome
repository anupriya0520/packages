package mypackage;
class Main {
    public static boolean isPalindrome(int num){
        int copy=num;
        int reverse=0;
        
        while(num>0){
            int digit=num%10;
            reverse=reverse*10+digit;
            num/=10;
        }
        if(copy==reverse){
            return true;
        }else{
        return false;
        }
    }
    public static void main(String[] args) {
        int n=1441;
        if(isPalindrome(n)){
            System.out.println("palindrome");
        }else{
            System.out.println("Not a palindrome");
        }
    }
}
