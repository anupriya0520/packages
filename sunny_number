class Main {
    
    public static void main(String[] args) {
        int n=8;
        int sunny=0;
        int sqrt=(int)Math.sqrt(n+1);
        sunny=sqrt*sqrt;
        if(sunny==n+1){
            System.out.println("Sunny number");
            
        }else{
            System.out.println("Not sunny");
        }
    }
}
